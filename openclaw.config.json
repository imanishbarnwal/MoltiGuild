{
    "models": {
        "providers": {
            "ollama": {
                "baseUrl": "https://ollama.com/v1",
                "apiKey": "8794f40c337a424ba82ea35df2aac2bf.PStbY6puC93px9EymmI0Ul8n",
                "models": [
                    {
                        "id": "gpt-oss:120b",
                        "name": "GPT-OSS 120B"
                    },
                    {
                        "id": "kimi-k2.5",
                        "name": "Kimi K2.5"
                    }
                ]
            }
        }
    },
    "agents": {
        "defaults": {
            "model": {
                "primary": "ollama/gpt-oss:120b"
            }
        },
        "list": [
            {
                "id": "coordinator",
                "name": "AgentGuilds Coordinator",
                "workspace": "~/.openclaw/workspace-coordinator",
                "tools": {
                    "allow": [
                        "exec",
                        "read",
                        "write",
                        "sessions_spawn",
                        "sessions_send",
                        "sessions_list",
                        "sessions_history",
                        "session_status"
                    ]
                }
            },
            {
                "id": "writer",
                "name": "Meme Writer",
                "workspace": "~/.openclaw/workspace-writer",
                "tools": {
                    "allow": [
                        "read"
                    ],
                    "deny": [
                        "exec",
                        "write",
                        "browser",
                        "sessions_spawn"
                    ]
                }
            },
            {
                "id": "director",
                "name": "Creative Director",
                "workspace": "~/.openclaw/workspace-director",
                "tools": {
                    "allow": [
                        "read"
                    ],
                    "deny": [
                        "exec",
                        "write",
                        "browser",
                        "sessions_spawn"
                    ]
                }
            }
        ]
    },
    "bindings": [
        {
            "agentId": "coordinator",
            "match": {
                "channel": "telegram"
            }
        }
    ],
    "channels": {
        "telegram": {
            "enabled": true,
            "botToken": "7715256287:AAF1-cTTqsJpVCHwDQmHo93n1juQ1dJXcbY",
            "dmPolicy": "open",
            "allowFrom": [
                "*"
            ],
            "groups": {
                "*": {
                    "requireMention": true
                }
            }
        }
    },
    "gateway": {
        "mode": "local",
        "controlUi": {
            "dangerouslyDisableDeviceAuth": true,
            "allowInsecureAuth": true
        }
    },
    "tools": {
        "agentToAgent": {
            "enabled": true,
            "allow": [
                "coordinator",
                "writer",
                "director"
            ]
        }
    },
    "plugins": {
        "entries": {
            "telegram": {
                "enabled": true
            }
        }
    }
}