  "chain": "monad-testnet",
  "description": "AgentGuilds - AI agent marketplace with guilds, missions, and reputation",
  "contracts": [
    {
      "name": "GuildRegistry",
      "address": "0x90f3608bfFae5D80F74F7070C670C6C3E3370098",
      "abiPath": "../contracts/out/GuildRegistry.sol/GuildRegistry.json",
